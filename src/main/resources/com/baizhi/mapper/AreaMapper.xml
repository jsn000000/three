<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "http://mybatis.org/dtd/mybatis-3-mapper.dtd" "mybatis-3-mapper.dtd" >
<mapper namespace="com.baizhi.dao.AreaDAO">
    <select id="selectByCity" resultType="Area">
        select id,name,code,cityid as cityCode  from Area where cityid=#{cityCode}
    </select>
</mapper>
